frontend-1                   | # Specify that we are a client and that we
frontend-1                   | # will be pulling certain config file directives
frontend-1                   | # from the server.
frontend-1                   | client
frontend-1                   | 
frontend-1                   | # Use the same setting as you are using on
frontend-1                   | # the server.
frontend-1                   | # On most systems, the VPN will not function
frontend-1                   | # unless you partially or fully disable
frontend-1                   | # the firewall for the TUN/TAP interface.
frontend-1                   | dev tun
frontend-1                   | 
frontend-1                   | # Are we connecting to a TCP or
frontend-1                   | # UDP server?  Use the same setting as
frontend-1                   | # on the server.
frontend-1                   | 
frontend-1                   | # Use TCP instead of UDP
frontend-1                   | proto tcp-client
frontend-1                   | 
frontend-1                   | 
frontend-1                   | # The hostname/IP and port of the server.
frontend-1                   | # You can have multiple remote entries
frontend-1                   | # to load balance between the servers.
frontend-1                   | remote vpn.example.org 1194
frontend-1                   | 
frontend-1                   | # Keep trying indefinitely to resolve the
frontend-1                   | # host name of the OpenVPN server.  Very useful
frontend-1                   | # on machines which are not permanently connected
frontend-1                   | # to the internet such as laptops.
frontend-1                   | resolv-retry infinite
frontend-1                   | 
frontend-1                   | # Most clients don't need to bind to
frontend-1                   | # a specific local port number.
frontend-1                   | nobind
frontend-1                   | 
frontend-1                   | # Try to preserve some state across restarts.
frontend-1                   | persist-key
frontend-1                   | persist-tun
frontend-1                   | 
frontend-1                   | # SSL/TLS parms.
frontend-1                   | # See the server config file for more
frontend-1                   | # description.  It's best to use
frontend-1                   | # a separate .crt/.key file pair
frontend-1                   | # for each client.  A single ca
frontend-1                   | # file can be used for all clients.
frontend-1                   | <ca>
